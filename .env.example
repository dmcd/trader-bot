# Core runtime defaults
TRADING_MODE=PAPER
ACTIVE_EXCHANGE=IB
LLM_PROVIDER=GEMINI
LLM_MODEL=gemini-2.5-flash

# LLM keys (set your own secrets; do not commit real values)
GEMINI_API_KEY=your_gemini_api_key
OPENAI_API_KEY=your_openai_api_key

# Interactive Brokers connection
IB_HOST=127.0.0.1
IB_PORT=7497
IB_CLIENT_ID=7
IB_ACCOUNT_ID=DU1234567
IB_PAPER=true
IB_BASE_CURRENCY=AUD
IB_EXCHANGE=SMART
IB_PRIMARY_EXCHANGE=ASX
IB_ALLOWED_INSTRUMENT_TYPES=STK,FX

# IB commissions
IB_STOCK_COMMISSION_PER_SHARE=0.005
IB_STOCK_MIN_COMMISSION=1.0
IB_FX_COMMISSION_PCT=0.00015

# IB test harness / playback
IB_TEST_MODE=playback
IB_PLAYBACK_FIXTURE=tests/fixtures/ib/playback_bundle.json
# Optional live gateway settings for manual fixture capture
IB_INTEGRATION_HOST=127.0.0.1
IB_INTEGRATION_PORT=7497
IB_INTEGRATION_CLIENT_ID=101
IB_INTEGRATION_ACCOUNT=DU1234567
